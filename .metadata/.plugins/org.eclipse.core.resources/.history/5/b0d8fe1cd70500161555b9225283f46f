package edu.milton.justin.biology.handlers;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileNotFoundException;
import java.io.FileReader;
import java.io.IOException;

public interface ReceptorHandler {

	File list = new File("Neurotransmitter Receptor List.txt");

	default void read() {
		
		String line = null;

		try {

			FileReader fileReader = new FileReader(list);

			BufferedReader bufferedReader = new BufferedReader(fileReader);

			while ((line = bufferedReader.readLine()) != null) {
				System.out.println(line);
			}

			bufferedReader.close();
			
		} catch (FileNotFoundException ex) {
			System.out.println("Unable to open list");
		} catch (IOException ex) {
			System.out.println("Error reading list");
		}

	}

}
